: ${ac_max_here_lines=38}

# Identity of this package.
PACKAGE_NAME='Yambo'
PACKAGE_TARNAME='yambo'
PACKAGE_VERSION='3.2.1 r.441 '
PACKAGE_STRING='Yambo 3.2.1 r.441 '
