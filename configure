ac_subst_vars='SHELL PATH_SEPARATOR PACKAGE_NAME PACKAGE_TARNAME PACKAGE_VERSION PACKAGE_STRING PACKAGE_BUGREPORT exec_prefix prefix program_transform_name bindir sbindir libexecdir datadir sysconfdir sharedstatedir localstatedir libdir includedir oldincludedir infodir mandir build_alias host_alias target_alias DEFS ECHO_C ECHO_N ECHO_T LIBS SVERSION SPATCHLEVEL SSUBLEVEL SREVISION CC CFLAGS LDFLAGS CPPFLAGS ac_ct_CC EXEEXT OBJEXT CPP EGREP ALLOCA LIBOBJS SET_MAKE build build_cpu build_vendor build_os host host_cpu host_vendor host_os hostname EDITOR ifGNUmake MAKE AR ac_ct_AR enable_debug enable_dp dp_cpp compile_p2y iotk_dir PW_VER PW_CPP compile_e2y etsf_dir MKMF_PREFIX FC FCFLAGS ac_ct_FC F90SUFFIX TIMER AR_FLAGS FCFLAGS_f90 FCFLAGS_f FCUFLAGS UFFLAGS FCMFLAG FCLIBS F90 C_AS_CPP C_AS_CPP_FLAGS F77 FFLAGS ac_ct_F77 FLIBS BLAS_LIBS LAPACK_LIBS compile_blas compile_lapack compile_local LOCAL_LIBS PFC MPICC mpi_cpp MPILIBS PF90 PF90FLAGS PCC PCCFLAGS mpibuild fft_cfactor FFTW_LIBS FFT_CPP FFT_DESCRIPTION BLACS_LIBS SCALAPACK_LIBS enable_scalapack dscalapack enable_largedb NCLIBS netcdf dnetcdf srcdir_path DP_str Red_str MPI_str NETCDF_str NETCDF_LF_str ETSF_str PW_str BLAS_str LAPACK_str LOCAL_str SLK_str LTLIBOBJS'
ac_subst_files=''

