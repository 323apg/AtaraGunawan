    if test -d "$with_etsf_io_libdir" ; then { $as_echo "$as_me:$LINENO: checking for ETSF_IO in $with_etsf_io_libdir" >&5
$as_echo_n "checking for ETSF_IO in $with_etsf_io_libdir... " >&6; } ; fi
    #
    if test -d "$with_etsf_io_path" ; then
        try_libdir=$with_etsf_io_path/lib
        try_incdir=$with_etsf_io_path/include
    fi
    if test -d "$with_etsf_io_libdir"     ; then try_libdir=$with_etsf_io_libdir ; fi
    if test -d "$with_etsf_io_includedir" ; then try_incdir=$with_etsf_io_includedir ; fi
    #
    if test -z "$try_libdir" ; then { { $as_echo "$as_me:$LINENO: error: No lib-dir specified" >&5
$as_echo "$as_me: error: No lib-dir specified" >&2;}
   { (exit 1); exit 1; }; } ; fi
    if test -z "$try_incdir" ; then { { $as_echo "$as_me:$LINENO: error: No include-dir specified" >&5
$as_echo "$as_me: error: No include-dir specified" >&2;}
   { (exit 1); exit 1; }; } ; fi
    #
    #
    if test -r $try_libdir/libetsf_io.a ; then
      compile_e2y="yes"
      compile_etsf="no"
      etsf_idir="$IFLAG$try_incdir"
      ETSF_LIBS="-letsf_io"
      #
      for file in `find $try_incdir \( -name '*etsf_io*' -o -name '*typesizes*' \) `; do
         cp $file include/
      done
      for file in `find $try_libdir -name '*etsf_io*.a'`; do
         cp $file lib/
      done
      #
      { $as_echo "$as_me:$LINENO: result: yes" >&5
$as_echo "yes" >&6; }
    else
      { $as_echo "$as_me:$LINENO: result: no" >&5
$as_echo "no" >&6; }
    fi
  elif test x"$with_etsf_io_libs" != "x" ; then
    #
    # directly provided lib
    #
    { $as_echo "$as_me:$LINENO: checking for ETSF_IO Library using $with_etsf_io_libs" >&5
$as_echo_n "checking for ETSF_IO Library using $with_etsf_io_libs... " >&6; }
    compile_etsf="no"
    compile_e2y="yes"
    if test -d "$with_etsf_io_includedir" ; then etsf_idir="$IFLAG$with_etsf_io_includedir" ; fi
    ETSF_LIBS="$with_etsf_io_libs"
    { $as_echo "$as_me:$LINENO: result: yes" >&5
$as_echo "yes" >&6; }
  else
    #
    # internal ETSF_IO
    #
    { $as_echo "$as_me:$LINENO: checking for ETSF_IO Library" >&5
$as_echo_n "checking for ETSF_IO Library... " >&6; }
    compile_etsf="yes"
    compile_e2y="yes"
    etsf_idir=" "
    ETSF_LIBS="-letsf_io"
    { $as_echo "$as_me:$LINENO: result: Internal" >&5
$as_echo "Internal" >&6; }
  fi
  #
else
  { $as_echo "$as_me:$LINENO: checking for ETSF_IO Library" >&5
$as_echo_n "checking for ETSF_IO Library... " >&6; }
  { $as_echo "$as_me:$LINENO: result: no" >&5
$as_echo "no" >&6; }
fi
