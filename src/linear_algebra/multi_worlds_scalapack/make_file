include ./make.sys

OBJS= \
kind.o \
SLK_tools.o \
error.o \
para.o \
constants.o \
SLK_ORTHO_group.o  SLK_ORTHO_init.o   SLK_POOLS_init.o \
util.o 

#
# main targets
#
all : mytest.x

