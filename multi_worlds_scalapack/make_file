
#
# config data
#
include ./make.sys

OBJS= \
kind.o \
distools.o \
error.o \
para.o \
constants.o \
mp_global.o \
util.o 

#
# main targets
#
all : mytest.x

mytest.x: mytest.o $(OBJS)
	$(LD) $(LDFLAGS) -o $@ mytest.o $(OBJS) $(LIBS)

# 
# deps 
# 
constants.o : kind.o
util.o : constants.o kind.o
mytest.o : $(OBJS)

#
# cleanup
#
clean:
	- rm -rf *.o *.a *.mod *.x *.f90
